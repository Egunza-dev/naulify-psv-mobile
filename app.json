{
  "expo": {
    "name": "Naulify",
    "slug": "naulify_v2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "naulifyv2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-foreground.png",
        "backgroundColor": "#2a2a2a"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.naulify_v2"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "serverClientId": "579728864798-1q9188ne549ogvh2bracdsrm7bu7rtct.apps.googleusercontent.com",
          "iosUrlScheme": "com.googleusercontent.apps.579728864798-078nlqmtvqums5mrfc62e1v8rgfc1q6k"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1eb68ca9-ad65-4310-acd2-4c23e09e283f"
      }
    }
  }
}
